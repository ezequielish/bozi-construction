{
  "name": "boziweb",
  "version": "1.0.0",
  "description": "Sito web de Construcciones y Reformas",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/index.js & npm run build:server:dev",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "start:dev": "webpack-dev-server --mode=development",
    "build:server:dev": "webpack --config ./webpack.server.config.js --mode=development",
    "build:server:prod": "NODE_ENV=production webpack --config ./webpack.server.config.js --mode=production",
    "server:dev": "BABEL_DISABLE_CACHE=1  babel-node ./server/server.js",
    "server:watch:babel": "babel-watch ./server/server.js",
    "build:dll": "webpack --config ./webpack.dll.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezequielish/bozi-construction.git"
  },
  "author": "@ezequiel_ish",
  "license": "MIT",
  "engines": {
    "node": "8.12.0"
  },
  "bugs": {
    "url": "https://github.com/ezequielish/bozi-construction/issues"
  },
  "homepage": "https://github.com/ezequielish/bozi-construction#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.3",
